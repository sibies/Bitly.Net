<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    
  </PropertyGroup>

	<PropertyGroup>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>

		<PackageIcon>bitly.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/sibies/Bitly.Net</RepositoryUrl>

		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageProjectUrl>https://sibies.ro</PackageProjectUrl>
		<Authors>Sibies Soft</Authors>
		<Company>Sibies Soft</Company>
		<Product>Bitly.Net</Product>
		<Copyright>Copyright © Sibies Soft</Copyright>
		<PackageTags>bitly short link</PackageTags>
		<NeutralLanguage>ro</NeutralLanguage>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\bitly.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

</Project>
